{
    "model_type": "inception_resnet_v2",
    "warm_start_from": "./model/embeddings/pre-trained/inception_resnet_v2_2016_08_30.ckpt",
    "learning_rate": 1e-5,
    "num_epochs": 1,

    "margin": 0.2,
    "triplet_strategy": "tf_batch_semi_hard",
    "squared": false,
    "use_batch_norm": true,

    "dataset": "imagenetvid",
    "image_size": 299,
    "image_channels": 3,

    "num_parallel_calls": 4,
    "save_summary_steps": 50,
    "save_checkpoints_steps": 200,
    "eval_interval_secs": 120,
    "eval_run_once": false,
    "eval_timeout_secs": 2500,

    "preprocess": true,
    "prefetch_threads": 4
}
